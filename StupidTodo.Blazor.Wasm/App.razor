<Fluxor.Blazor.Web.StoreInitializer />

<Router AppAssembly="@typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(Main).Assembly }" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(Main)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(Main)">
            <p>Empty Route</p>
        </LayoutView>
    </NotFound>
</Router>
